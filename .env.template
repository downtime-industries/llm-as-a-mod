# Discord token
DISCORD_TOKEN=your_discord_token_here

# Discord intents (as an integer)
# 292070435846 includes common intents like messages, guilds, members, etc.
DISCORD_INTENTS=292070435846

# Ollama API base URL (defaults to localhost if not specified)
# Examples:
# OLLAMA_BASE_URL=http://localhost:11434  (default)
# OLLAMA_BASE_URL=http://your-server:11434  (external server)
OLLAMA_BASE_URL=http://localhost:11434

# Logging level (string), e.g., DEBUG, INFO, WARNING
LOG_LEVEL=INFO